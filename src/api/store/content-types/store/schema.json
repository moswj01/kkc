{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store_name": {
      "type": "string"
    },
    "lat": {
      "type": "float"
    },
    "lng": {
      "type": "float"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "machines": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::machine.machine",
      "mappedBy": "store"
    }
  }
}
